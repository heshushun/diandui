<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<!-- 
    积分记录对象 （映射配置文件  对应一个表）
-->
<hibernate-mapping>
    <class name="com.diandui.model.Jifen" table="jifen">
        <id name="jifenId">
            <column name="jifen_id" />
            <generator class="identity"></generator>
        </id>
        
        <property name="userId" type="java.lang.Integer">
            <column name="user_id" />
        </property>

        <property name="jifenDate" type="java.sql.Date">
            <column name="jifen_date" />
        </property>
        
        <property name="jifenFrom" type="java.lang.String">
            <column name="jifen_from" />
        </property>
        
        <property name="jifenFromId" type="java.lang.Integer">
            <column name="jifen_from_id" />
        </property>
        
        <property name="jifenValue" type="java.lang.Integer">
            <column name="jifen_value" />
        </property>
                   
    </class>
</hibernate-mapping>
