<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<!-- 
    押注对象 （映射配置文件  对应一个表）
-->
<hibernate-mapping>
    <class name="com.diandui.model.Battle" table="battle">
        <id name="battleId">
            <column name="battle_id" />
            <generator class="identity"></generator>
        </id>
        
        <property name="userId" type="java.lang.Integer">
            <column name="user_id" />
        </property>

        <property name="bettlePlateId" type="java.lang.Integer">
            <column name="bettlePlate_id" />
        </property>
        
        <property name="stockId" type="java.lang.String">
            <column name="stock_id" />
        </property>
        
        <property name="stockName" type="java.lang.String">
            <column name="stock_name" />
        </property>
        
        <property name="bettlePlateDate" type="java.sql.Date">
            <column name="bettlePlate_date" />
        </property>
        
        <property name="battleJifen" type="java.lang.Integer">
            <column name="battle_jifen" />
        </property>
        
        <property name="battleType" type="java.lang.Integer">
            <column name="battle_type" />
        </property>
        
        <property name="riseDropType" type="java.lang.Integer">
            <column name="rise_drop_type" />
        </property>
        
        <property name="battleResult" type="java.lang.Integer">
            <column name="battle_result" />
        </property>
        
        <property name="battleGetJifen" type="java.lang.Integer">
            <column name="battle_get_jifen" />
        </property>
        
        
    </class>
</hibernate-mapping>
